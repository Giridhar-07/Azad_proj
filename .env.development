# Development environment variables for Vite

# Base URL for assets
VITE_BASE_URL=/

# Disable URI encoding for assets
VITE_DISABLE_URI_ENCODING=true

# Force Vite to use strict URL handling
VITE_STRICT_URL=true

# Set the port for the development server
VITE_PORT=3000

# Set the proxy target for API requests
VITE_PROXY_TARGET=http://localhost:8000